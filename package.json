{
  "name": "@saschazar/unicat-icons",
  "version": "0.0.1",
  "description": "An SVG collection for React, React Native and react-sketchapp",
  "main": "index.min.js",
  "module": "index.es.min.js",
  "browser": "index.umd.min.js",
  "scripts": {
    "build:empty": "node lib/emptier",
    "build:transpile": "TARGET=web node lib/executor && TARGET=native node lib/executor && TARGET=sketchapp node lib/executor",
    "build:fix-imports": "tslint --config lib/configs/tslint-fix-imports.json --fix --project .",
    "build:compile": "TARGET=web rollup -c && TARGET=native rollup -c && TARGET=sketchapp rollup -c",
    "build:copy": "shx cp package.json README.md CHANGELOG.md dist/",
    "build:prepare": "npm run build:empty && npm run build:transpile && npm run build:fix-imports",
    "build": "npm run build:prepare && npm run build:compile && npm run build:copy",
    "docs:prepare": "node lib/docs",
    "docs": "npm run docs:prepare && next",
    "docs:build": "npm run docs:prepare && next build"
  },
  "repository": "https://github.com/saschazar21/unicat-icons",
  "author": "Sascha Zarhuber <sascha.zarhuber@gmail.com> (https://sascha.work/)",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "icon",
    "svg",
    "react",
    "react-native",
    "react-sketchapp",
    "sketch",
    "typescript"
  ],
  "peerDependencies": {
    "react": "^16.9.0",
    "react-native": "^0.61.1",
    "react-native-svg": "^9.9.6",
    "react-sketchapp": "^3.0.0-beta.9"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@mdx-js/loader": "^1.0.21",
    "@mdx-js/mdx": "^1.0.21",
    "@svgr/core": "^4.3.0",
    "@svgr/plugin-jsx": "^4.3.0",
    "@svgr/plugin-prettier": "^4.2.0",
    "@svgr/plugin-svgo": "^4.2.0",
    "@types/node": "^12.6.2",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-native": "^0.57.60",
    "@types/react-sketchapp": "^0.16.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-mdx": "^1.2.0",
    "autoprefixer": "^9.6.0",
    "fs-extra": "^8.0.1",
    "globby": "^9.2.0",
    "next": "^9.0.6",
    "next-compose-plugins": "^2.2.0",
    "param-case": "^2.1.1",
    "pascal-case": "^2.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-native": "^0.61.1",
    "react-native-svg": "^9.9.6",
    "react-sketchapp": "^3.0.0-beta.9",
    "rehype-highlight": "^3.0.0",
    "remark-emoji": "^2.0.2",
    "remark-images": "^0.18.0",
    "rollup": "^1.14.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-filesize": "^6.1.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-typescript2": "^0.21.1",
    "shx": "^0.3.2",
    "tslint": "^5.17.0",
    "tslint-etc": "^1.5.3",
    "typescript": "^3.5.1"
  }
}
